<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="ymz.coffeerep.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_beansListFragment"
            app:destination="@id/beansListFragment" />
    </fragment>
    <fragment
        android:id="@+id/beansListFragment"
        android:name="ymz.coffeerep.scenes.list.BeansListFragment"
        android:label="BeansListFragment" >
        <action
            android:id="@+id/action_beansListFragment_to_rawBeansInsertFragment"
            app:destination="@id/rawBeansInsertFragment" />
        <action
            android:id="@+id/action_beansListFragment_to_rawBeansDetailFragment"
            app:destination="@id/rawBeansDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/rawBeansInsertFragment"
        android:name="ymz.coffeerep.scenes.insert.RawBeansInsertFragment"
        android:label="RawBeansInsertFragment" >
        <action
            android:id="@+id/action_rawBeansInsertFragment_to_beansListFragment"
            app:destination="@id/beansListFragment" />
    </fragment>
    <fragment
        android:id="@+id/rawBeansDetailFragment"
        android:name="ymz.coffeerep.scenes.detail.RawBeansDetailFragment"
        android:label="RawBeansDetailFragment" />
</navigation>